<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">
	
	<menu id="root-menu" label="Openbox">
		
		<menu id="System" label="System">
			<item label="Terminal">
			    <action name="Execute">
					<command>urxvt</command>
				</action>
			</item>
			<item label="File Manager">
			    <action name="Execute">
					<command>thunar</command>
				</action>
			</item>
			<item label="Reconfigure Openbox">
				<action name="Reconfigure" />
			</item>
		</menu>	

		<menu id="Internet" label="Internet">
			<item label="Chromium">
				<action name="Execute">
					<command>chromium</command>
					<startupnotify>
						<enabled>yes</enabled>
						<wmclass>Chrome</wmclass>
					</startupnotify>
				</action>
			</item>
			<item label="dwb">
				<action name="Execute">
					<command>dwb</command>
					<startupnotify>
						<enabled>yes</enabled>
					</startupnotify>
				</action>
			</item>
		</menu>

		<menu id="Media" label="Media">
			<item label="Spotify">
				<action name="Execute">
					<command>spotify</command>
					<startupnotify>
						<enabled>yes</enabled>
					</startupnotify>
				</action>
			</item>
		</menu>		
	
		<menu id="Games" label="Games">
			<item label="steam">
				<action name="Execute">
					<command>steam</command>
					<startupnotify>
						<enabled>yes</enabled>
					</startupnotify>
				</action>
			</item>
			<item label="minecraft">
				<action name="Execute">
					<command>minecraft</command>
				</action>
			</item>
		</menu>		
		<menu id="Graphics" label="Graphics">
			<item label="Gimp">
				<action name="Execute">
					<command>gimp</command>;
					<startupnotify>
						<enabled>yes</enabled>
					</startupnotify>
				</action>
			</item>
		</menu>

	</menu>


</openbox_menu>
